def menu():
    condition = True
    while condition:
        print(" ** Course Registration System  **")
        print("1) Enroll")
        print("2) Drop")
        print("3) Search")
        print("4) View All")
        print("5) Sort")
        print("6) Exit")
        choice = int(input("Enter a choice: "))
        match choice:
            case 1:
                enroll()
            case 2:
                drop()
            case 3:
                search()
            case 4:
                view()
            case 5:
                sort()
            case 6:
                condition = False
                print("Exiting the system.")
            case _:
                print("Invalid ...................")
